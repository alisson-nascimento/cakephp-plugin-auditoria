{
    "name": "gonube/cakephp-plugin-auditoria",
    "description": "Auditoria plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
		"maiconpinto/cakephp-adminlte-theme": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Auditoria\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auditoria\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
